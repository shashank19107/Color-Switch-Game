<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="startMenuScreen" prefHeight="770.0" prefWidth="1040.0" style="-fx-background-color: BLACK;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.StartMenu">
   <children>
      <Button alignment="CENTER" layoutX="463.0" layoutY="229.0" mnemonicParsing="false" prefHeight="56.0" prefWidth="145.0" text="Resume Game" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS" />
      <Button alignment="CENTER" layoutX="463.0" layoutY="330.0" mnemonicParsing="false" prefHeight="56.0" prefWidth="145.0" text="High Scores" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS" />
      <Button alignment="CENTER" layoutX="463.0" layoutY="430.0" mnemonicParsing="false" prefHeight="56.0" prefWidth="145.0" text="Exit Game" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS" />
      <ImageView fitHeight="56.0" fitWidth="71.0" layoutX="370.0" layoutY="229.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/load(3).png" />
         </image>
      </ImageView>
      <ImageView fitHeight="56.0" fitWidth="65.0" layoutX="364.0" layoutY="330.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/high-score.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="56.0" fitWidth="90.0" layoutX="364.0" layoutY="430.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/exit.png" />
         </image>
      </ImageView>
      <ImageView fx:id="newGameIcon" fitHeight="57.0" fitWidth="56.0" layoutX="364.0" layoutY="130.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/console.png" />
         </image>
      </ImageView>
      <Button fx:id="newGameButton" alignment="CENTER" layoutX="463.0" layoutY="130.0" mnemonicParsing="false" onAction="#openGamePlay"  onMouseEntered="#flipNewGameIcon" prefHeight="56.0" prefWidth="145.0" text="New Game" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS" />
   </children>
</AnchorPane>
