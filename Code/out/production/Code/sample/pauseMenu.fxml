<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="GamePlayRoot" prefHeight="800.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.PauseMenu">
   <children>
      <ImageView fx:id="Background" fitHeight="800.0" fitWidth="1024.0" nodeOrientation="INHERIT" pickOnBounds="true">
         <image>
            <Image url="@../assets/Pause_menu_logo.png" />
         </image>
      </ImageView>
      <Button layoutX="137.0" layoutY="531.0" mnemonicParsing="false" onAction="#play" onMouseClicked="#play" prefHeight="36.0" prefWidth="122.0" text="Resume Game" textAlignment="CENTER" />
      <Button layoutX="337.0" layoutY="531.0" mnemonicParsing="false" onAction="#save" prefHeight="36.0" prefWidth="87.0" text="Save" />
      <Button layoutX="551.0" layoutY="531.0" mnemonicParsing="false" onAction="#exit" prefHeight="36.0" prefWidth="87.0" text="Exit" />
      <Button layoutX="764.0" layoutY="531.0" mnemonicParsing="false" onAction="#Enterplayernamescreen" prefHeight="36.0" prefWidth="107.0" text="Do Special" />
   </children>
</AnchorPane>
